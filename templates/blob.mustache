{{> top }}
{{> nav }}

<div id="wrapper"> 
<div id="main">

<h2>
{{#breadcrumbs}}
{{#href}}<a href="{{href}}">{{/href}}{{name}}{{#href}}</a>{{/href}}&nbsp;{{#dir}}/&nbsp;{{/dir}}
{{/breadcrumbs}}
</h2>

<div id="actions">
    <a href="{{rawUrl}}">raw</a>
</div>

{{#text}}
<table cellpadding="0" cellspacing="0" width="100%" class="code"><tbody><tr>
<td><pre class="linenums">{{#code}}<span id="L{{n}}" rel="#L{{n}}">{{n}}</span>
{{/code}}</pre></td>
<td width="100%"><pre class="codetext">{{#code}}<div class="line" id="LC{{n}}" style="background-color: transparent; ">{{line}}<br/></div>{{/code}}</pre></td>
</tr></tbody></table>
{{/text}}
{{^text}}
{{#isImage}}
<img src="{{rawUrl}}">
{{/isImage}}
{{^isImage}}
<p>Binary content. <!-- TODO: download link --></p>
{{/isImage}}
{{/text}}

</div> <!-- end of #wrapper -->
</div> <!-- end of #main -->
{{> bottom }}
